{
  "build" : {
    "-lib" : [
      "tink_core",
      "stx_pico",
      "stx_nano",
      "stx_log",
      "stx_fn",
      "stx_proxy",
      "stx_coroutine",
      "stx_parse",
      "bake",
      "fletcher",
      "pml"
    ],
    "-D"    : [
      "no-deprecation-warnings",
      "stx.log.filter.show=false",
      "haxe.issue_9359"
    ],
    "-cp" : "src/main/haxe",
    "unit" : {
      "--macro" : [
        "include('stx.io',true)",
        "include('stx.asys.fs',true)",
        "include('stx.asys',true)",
        "include('stx.parse',true)",
        "include('stx.assert',true)",
        "include('stx.proc',true)"
      ],
      "interp" : {
        "--interp" : true
      },
      "javascript" : {
        "--macro" : ["exclude('stx.asys',true)"],
        "--js" : "build/javascript/unit.js"
      }
    },
    "stx.asys.fs.path" : {
      "interp" : {
        "test" : {
          "-lib" : ["stx_test"],
          "-main" : "stx.asys.fs.path.Test"
        },
        "--interp" : true
      },
      "neko" : {
        "test" : {
          "-lib" : ["stx_test"],
          "-main" : "stx.asys.fs.path.Test",
          "-neko" : "build/neko/test/main.n"
        }
      }
    },
    "stx.parse.path" : {
      "test" : {
        "-lib" : ["stx_test"],
        "-main" : "stx.parse.path.Test",
        "interp" : {
          "--interp" : true
        }
      }
    },
    "stx.io" : {
      "--resource" : "tests.pml@tests",
      "test" : {
        "-lib" : ["stx_test"],
        "-main" : "stx.io.Test",
        "interp" : {
          "--interp" : true
        }
      }
    },
    "feat" : {
      "-cp"  : "src/test/haxe",
      "-lib" : ["stx_test"],
      "fs.path" : {
        "-main" : "stx.asys.fs.path.Test",
        "interp" : {
          "--interp" : true
        }
      }
    }
  },
  "run" : {
    "stx.asys.fs.path/test/neko:run" : [
      ["neko","build/neko/test/main.n"]
    ]
  } 
}